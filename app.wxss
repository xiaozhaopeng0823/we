@import './styles/iconfont.wxss';
/* 小程序中 不支持通配符 */
page,
view,
text,
swiper,
swiper-item,
image,
navigator {
	padding: 0;
	margin: 0;
	box-sizing: border-box;
}

image {
	vertical-align: middle;
}
page {
	--themeColor:#ff2d4a;
}

.line2 {
   display: -webkit-box;
   overflow: hidden;
   -webkit-box-orient: vertical;
   -webkit-line-clamp: 2;
   word-break: break-all;
}
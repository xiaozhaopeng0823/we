swiper {
  height: 340rpx;
  width: 100%;
}
swiper image {
  width: 100%;
  height: 340rpx;
}
.nav {
  display: flex;
  justify-content: space-around;
  margin-top: 24rpx;
  margin-bottom: 29rpx;
  background-color: #fff;
}
.nav .navitem image {
  width: 128rpx;
  height: 128rpx;
}
.floor {
  margin-bottom: 20rpx;
}
.floor .floor-title {
  width: 100%;
  margin-bottom: 20rpx;
}
.floor .floor-title image {
  width: 100%;
  height: 118rpx;
  background-color: #f4f4f4;
}
.floor .floor-body {
  padding: 0 16rpx;
  display: flex;
}
.floor .floor-body .left {
  height: 386rpx;
  width: 232rpx;
}
.floor .floor-body .left image {
  height: 386rpx;
  width: 232rpx;
}
.floor .floor-body .right {
  margin-left: 10rpx;
  display: flex;
  flex: 1;
  flex-wrap: wrap;
  justify-content: space-between;
  align-content: space-between;
}
.floor .floor-body .right image {
  width: 233rpx;
  height: 188rpx;
}
.gotop {
  position: fixed;
  right: 30rpx;
  bottom: 20rpx;
  height: 120rpx;
  width: 120rpx;
  background-color: #fff;
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
